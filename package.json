{
  "name": "arcgis",
  "description": "Node client library for ArcGIS",
  "version": "0.0.1",
  "author": "Nikolas Wise <nikolaswise@gmail.com>",
  "bugs": {
    "url": "https://github.com/esripdx/node-arcgis/issues"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.4.0",
    "browserify": "^12.0.0",
    "faucet": "0.0.1",
    "gfm.css": "^1.1.1",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "rerun-script": "^0.6.0",
    "sanitize-html": "^1.11.1",
    "standard": "*",
    "static-site": "^1.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "unique": "0.0.1",
    "watchify": "^3.5.0"
  },
  "homepage": "https://github.com/nikolaswise/node-arcgis",
  "keywords": [
    "arcgis",
    "gis"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaswise/node-arcgis.git"
  },
  "watches": {
    "doc": [
      "*.md",
      "_layout.html"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "predev": "npm run doc",
    "dev": "parallelshell 'npm run server' 'npm run js' 'rerun-script'",
    "doc": "static-site -s ./ -b ./docs -i node_modules/ docs/ --template _layout.html",
    "postdoc": "cp ./node_modules/gfm.css/gfm.css ./docs",
    "predist": "mkdirp dist/node dist/browser",
    "dist": "browserify lib/index.js -t babelify --outfile dist/browser/index.js",
    "postdist": "babel lib --out-dir dist/node",
    "build": "parallelshell 'npm run dist' 'npm run docs'",
    "server": "http-server docs/",
    "js": "watchify test/index.js -t babelify --outfile docs/bundle.js",
    "predeploy": "browserify lib/index.js -t babelify --outfile docs/arcgis.js",
    "deploy": "surge docs/ --domain node-arcgis.surge.sh",
    "test": "faucet"
  }
}
